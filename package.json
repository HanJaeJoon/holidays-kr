{
	"name": "@hyunbinseo/holidays-kr",
	"version": "2.2024.4",
	"description": "South Korea's national holidays, based on the official gazette",
	"scripts": {
		"build": "esno cli/generate && tsup index.ts --format cjs,esm --dts --clean"
	},
	"devDependencies": {
		"@types/node": "^20.12.5",
		"esno": "^4.7.0",
		"prettier": "^3.2.5",
		"sendgrid-send": "^1.0.7",
		"tsup": "^8.0.2",
		"typescript": "^5.4.4"
	},
	"files": [
		"dist",
		"public",
		"!public/_redirects"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		}
	},
	"keywords": [
		"대한민국",
		"한국",
		"공휴일",
		"Korean",
		"holidays"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hyunbinseo/holidays-kr.git"
	},
	"author": "Hyunbin Seo",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/hyunbinseo/holidays-kr/issues"
	},
	"homepage": "https://github.com/hyunbinseo/holidays-kr#readme"
}
